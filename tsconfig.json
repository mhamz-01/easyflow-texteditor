{
    "compilerOptions": {
      "incremental": false,
      "declaration": true,
      "declarationMap": true,
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "node",
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["src"]
  }
  